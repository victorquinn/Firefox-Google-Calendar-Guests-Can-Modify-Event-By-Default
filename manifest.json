{
    "manifest_version": 2,
    "name": "Firefox Google Calendar Guests Modify Default",
    "version": "0.2",
    "applications": {
        "gecko": {
            "id": "firefox-google-calendar-guests-modify-default@victorquinn.com"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "https://calendar.google.com/*",
                "https://www.google.com/calendar/*"
            ],
            "js": [
                "/js/calendar-script.js"
            ]
        }
    ]
}
